
@*@{
    ViewBag.Title = "Index";
}*@

@model IEnumerable <mvc.Models.Teacher>
<div>
    @*<b>Teacher ID </b> @Model.TeacherId <br />
        <b>Name</b> @Model.Name <br />
        <b>Class</b>@Model.Class <br />
        <b> Salary</b>@Model.Salary <br />*@
    <h1 class="text-primary text-center"> List of Teacher</h1>
    <table class="table table-bordered table-striped table-hover">
        <thead>
            <tr>
                <th>Teacher ID </th>
                <th>Name</th>
                <th>Class</th>
                <th>Salary</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var teach in Model)
            {
                <tr>
                    <td>@teach.TeacherId</td>
                    <td>@teach.Name</td>
                    <td>@teach.Class</td>
                    <td>@teach.Salary</td>
                </tr>
            }
        </tbody>
    </table>

</div>

<h2>Index</h2>

